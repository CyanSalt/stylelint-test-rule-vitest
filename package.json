{
  "name": "stylelint-test-rule-vitest",
  "version": "0.1.1",
  "description": "A Stylelint rule tester using Vitest",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --fix './**/*.{js,ts}'",
    "prepack": "tsdown",
    "release": "release-it",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/stylelint-test-rule-vitest.git"
  },
  "keywords": [
    "stylelint",
    "test",
    "vitest"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyanSalt/stylelint-test-rule-vitest/issues"
  },
  "homepage": "https://github.com/CyanSalt/stylelint-test-rule-vitest#readme",
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.3.0",
    "@cyansalt/tsconfig": "^3.0.0",
    "@release-it/conventional-changelog": "^9.0.4",
    "eslint": "^9.17.0",
    "release-it": "^18.0.0",
    "stylelint": "^16.12.0",
    "tsdown": "^0.16.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.1",
    "vitest": "*"
  }
}
